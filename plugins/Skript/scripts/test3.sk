on right click with bone:
	if player's tool is a bone:
		if player's tool is a bone:
			set {_i} to player's tool
			set {_in} to {_i}'s name
			set {_il::*} to {_i}'s lore
			set {_inbt} to {_i}'s nbt
			set player's tool to ghast tear with nbt "%{_inbt}%"
			set {_id} to player's tool
			SB_ItemAbility_Throwing(player, "", 0, 100, 1.3, bone, "hand", 1.25, -1.25, "y", 20, 0, "", 0)
			wait 1.3 second
			loop all items in player's inventory:
				if loop-item is {_id}:
					if loop-item's nbt is {_i}'s nbt:
						set loop-item to {_i}
	
function SB_ItemAbility_Throwing(p: player, name: text, dmg: number, pdmg: number, r: number, i: item, h: text, w: number, t: number, ri: text, rv: number, type: number, sound: text, psound: number):
	# dmg: -2= n/a -1= abilitydmg 0= weapondmg / r: radius / h: head/hand / w: wait while kill / t: spd / ri: rotate x y z? / rv: rotate spd / type: 0=no explode, 1= yes
	set {_toolid} to {_p}'s tool
	set {_lr} to 0
	if {_h} is not "head":
		set {_lr} to 0.5
	set {_l} to location {_lr} left {_p}'s location
	set {_l} to location ({_lr}+0.25) meters above {_l}
	spawn an armor stand at {_l}
	set {_s} to the spawned entity
	set name of {_s} to "✧%{_name}%✧"
	add "{Invisible:1b,NoGravity:1b,ShowArms:1b,Marker:1b,DisabledSlots:63}" to nbt of {_s}
	if {_h} is "hand":
		set weapon of {_s} to {_i}
		set {_h} to "RightArm"
	if {_h} is "head":
		set {_s}'s helmet to {_i}
		set {_h} to "Head"
	WaitKill({_p}, {_s}, {_w})
	loop 50 times:
		if {_s} is not alive:
			kill {_s}
			stop
		loop all living entities in radius {_r} of {_s}:
			if loop-entity is not a player:
				if loop-entity is not an armor stand:
					if {_o::*} contains loop-entity:
						remove loop-entity from {_o::*}
					else:
						add loop-entity to {_o::*}
						if {_dmg} is equal to 0:
							MakeDamage({_p}, loop-entity, {_pdmg})
						else if {_dmg} is equal to -1:
							MakeMagicDamage({_p}, loop-entity, {_pdmg})
						else if {_dmg} is equal to -2:
							continue
						else:
							MakeSpecificDamage({_p}, loop-entity, {_dmg}, {_pdmg}, 3)
						add loop-entity to {_o::*}
						if {_type} is equal to 1:
							play sound "%{_sound}%" with volume 4 with pitch {_psound} at {_s} for players
							kill {_s}
		if block 1 meters above {_s}'s location is not air:
			if {_type} is equal to 1:
				play sound "%{_sound}%" with volume 4 with pitch {_psound} at {_s} for players
				set {_pt} to {_p}'s targeted block
				loop all players in radius 5 of {_s}:
					if loop-player-1 is {_p}:
						spawn an armor stand at {_pt}'s location
						set {_ss} to the spawned entity
						loop all players in radius 5 of {_ss}:
							if loop-player-2 is {_p}:
								spawn an armor stand -2 meters horizontally in front of {_p}
								set {_sss} to the spawned entity
								loop all living entities in radius 3.5 of {_sss}:
									if loop-entity-3 is {_ss}:
										PushExplode({_p})
										kill {_sss}
										kill {_ss}
										kill {_s}
										stop	
								push {_p} backwards at speed 1
								push {_p} upward at speed 0.1
								kill {_sss}
								kill {_ss}
								kill {_s}
								stop
			kill {_sss}
			kill {_ss}
			kill {_s}
			stop
		if {_toolid} is a bone or ghast tear:
			if {_tt} is higher than 13.9:
				set {_t} to 1.5
				set {_pdmg} to 200
				delete {_o::*}
			else:
				add 1 to {_tt}
		teleport {_s} {_t} meters behind {_s}
		if {_ri} is "x":
			add {_rv} to {_v}
			add "{Pose:{%{_h}%:[%{_v}%f,0f,0f]}}" to nbt of {_s}
		if {_ri} is "y":
			add {_rv} to {_v}
			add "{Pose:{%{_h}%:[0f,%{_v}%f,0f]}}" to nbt of {_s}
		if {_ri} is "z":
			add {_rv} to {_v}
			add "{Pose:{%{_h}%:[0f,0f,%{_v}%f]}}" to nbt of {_s}
		wait 1 tick
	kill {_s}
	stop