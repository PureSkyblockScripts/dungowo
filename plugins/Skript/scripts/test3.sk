on right click with stick:
	if name of player's tool contains "&6Heroic Ice Spray Wand":
		SB_ItemManaCost(player, 50, "R")
		SB_ItemCooldown(player, 5, "R")
		if {SB-ItemAble.%UUID of player%} is true:
			SB_AbilityUsed(player, "Ice Spray")
			SB_ItemAbility_Throwing(player, "&b&lICESPRAYWAND", -2, 0, 1.1, ice, "head", 0.45, -1, "y", 0, 0, "", 0)
			wait 1 tick
			play 50 of snowflake at player's location
			loop all living entities in radius 5 of player:
				if loop-entity's name is "&b&lICESPRAYWAND":
					set {_v} to loop-entity
			loop 5 times:
				loop all living entities in radius 1.5 of {_v}:
					if loop-entity is not an armor stand:
						if loop-entity is not a player:
							apply slowness 250 without particles to loop-entity for 5 seconds replacing existing effect
							wait 0.2 second

