				set {SellItem-Item.%UUID of player%} to name of clicked item
				
			else if name of clicked item is "&9Canne à Sucre Enchanté":
				set {SellItem-Price.%UUID of player%} to 51000
				set {SellItem-Item.%UUID of player%} to name of clicked item
				
			else if name of clicked item is "&9Bloc de Redstone Enchanté" or "&9Bloc de Lapis Enchanté" or "&9Pastèque Enchanté":
				set {SellItem-Price.%UUID of player%} to 25600
				set {SellItem-Item.%UUID of player%} to name of clicked item
				
			else if name of clicked item is "&9Bloc de Fer Enchanté":
				set {SellItem-Price.%UUID of player%} to 76800
				set {SellItem-Item.%UUID of player%} to name of clicked item
				
			else if name of clicked item is "&9Bloc de Champignon Rouge Enchanté" or "&9Bloc de Champignon Marron Enchanté" or "&9Bloc d'Or Enchanté":
				set {SellItem-Price.%UUID of player%} to 153600
				set {SellItem-Item.%UUID of player%} to name of clicked item
			
			else if name of clicked item is "&9Bloc d'Émeraude Enchanté":
