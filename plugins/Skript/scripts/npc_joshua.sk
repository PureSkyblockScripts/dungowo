on right click on entity:
 if entity's name is "&7Joshua":
  make console execute command "/joshua %player%"

command /joshua <player>:
	permission: op
	trigger:
		if {abc} is not true:
			if {abc} is not true:
				open virtual chest inventory with size 4 named "Joshua" to arg-1
				format gui slot (integers between 0 and 35) of arg-1 with black stained glass pane named "&0"

				format gui slot (integers between 1 and 5) of arg-1 with gray stained glass pane named "&0"
				format gui slot 1 of arg-1 with lime stained glass pane named "&0"

				format gui slot 31 of arg-1 with barrier named "&cFermer &0" to close
				
				set {SellItem-Mode-Normal.%UUID of arg-1%} to true
				set {SellItem-Mode-Vision.%UUID of arg-1%} to false
				set {SellItem-Mode-All.%UUID of arg-1%} to false
				set {SellItem-Mode-Size.%UUID of arg-1%} to 1
				set {SellItem-Price.%UUID of arg-1%} to 0
				set {SellItem-Item.%UUID of arg-1%} to air named "&cRien"
				
				format gui slot 10 of arg-1 with gold nugget named "&aVendre par x1 &0" with lore "&7Vendez vos articles à une quantité" and "&7de &cx1 &7lorsque vous cliquez dessus." and "&7" and "&eCliquez pour sélectionner!" to run:
					format gui slot (integers between 1 and 5) of player with gray stained glass pane named "&0"
					format gui slot 1 of player with lime stained glass pane named "&0"
					set {SellItem-Mode-All.%UUID of player%} to false
					set {SellItem-Mode-Size.%UUID of player%} to 1
					play sound "ui.button.click" at player for player
					stop

				format gui slot 11 of arg-1 with gold ingot named "&aVendre par x16 &0" with lore "&7Vendez vos articles à une quantité" and "&7de &cx16 &7lorsque vous cliquez dessus." and "&7" and "&eCliquez pour sélectionner!" to run:
					format gui slot (integers between 1 and 5) of player with gray stained glass pane named "&0"
					format gui slot 2 of player with lime stained glass pane named "&0"
					set {SellItem-Mode-All.%UUID of player%} to false
					set {SellItem-Mode-Size.%UUID of player%} to 16
					play sound "ui.button.click" at player for player
					stop
				
				format gui slot 12 of arg-1 with gold block named "&aVendre par x32 &0" with lore "&7Vendez vos articles à une quantité" and "&7de &cx32 &7lorsque vous cliquez dessus." and "&7" and "&eCliquez pour sélectionner!" to run:
					format gui slot (integers between 1 and 5) of player with gray stained glass pane named "&0"
					format gui slot 3 of player with lime stained glass pane named "&0"
					set {SellItem-Mode-All.%UUID of player%} to false
					set {SellItem-Mode-Size.%UUID of player%} to 32
					play sound "ui.button.click" at player for player
					stop

				format gui slot 13 of arg-1 with gold block of unbreaking 1 with hidden enchants flag named "&aVendre par x64 &0" with lore "&7Vendez vos articles à une quantité" and "&7de &cx64 &7lorsque vous cliquez dessus." and "&7" and "&eCliquez pour sélectionner!" to run:
					format gui slot (integers between 1 and 5) of player with gray stained glass pane named "&0"
					format gui slot 4 of player with lime stained glass pane named "&0"
					set {SellItem-Mode-All.%UUID of player%} to false
					set {SellItem-Mode-Size.%UUID of player%} to 64
					play sound "ui.button.click" at player for player
					stop
				
				format gui slot 14 of arg-1 with yellow stained glass of unbreaking 1 with hidden enchants flag named "&aVendre tout &0" with lore "&7Vendez tout l'article en question" and "&7présents dans votre inventaire." and "&7" and "&eCliquez pour sélectionner!" to run:
					format gui slot (integers between 1 and 5) of player with gray stained glass pane named "&0"
					format gui slot 5 of player with lime stained glass pane named "&0"
					set {SellItem-Mode-All.%UUID of player%} to true
					set {SellItem-Mode-Size.%UUID of player%} to 1
					play sound "ui.button.click" at player for player
					stop
				
				format gui slot 16 of arg-1 with birch sign named "&aFonctionnement des ventes &0" with lore "&c&lINFO!" and "&7Cliquez l'article que vous souhaitez vendre" and "&7dans votre inventaire pour récupérer des" and "&7pièces en échange." and "&7" and "&6&lOPTION!" and "&7Choissisez votre mode de quantité à vendre" and "&7lorsque vous cliquerez sur un article." and "&7" and "&9&lPRIX!" and "&7Informez vous sur le prix en activant le mode" and "&7vision ci-dessous. Ensuite, cliquez sur l'article" and "&7que vous souhaitez voir le prix."

				format gui slot 33 of arg-1 with ender eye named "&9Mode Vision &0" with lore "&7Cliquez sur l'article en question pour" and "&7voir le prix du vente." and "&7" and "&eCliquez pour changer mode!" to run:
					set {SellItem-Mode-Normal.%UUID of player%} to false
					set {SellItem-Mode-Vision.%UUID of player%} to true
					play sound "block.chest.open" at player for player
					stop
				
				format gui slot 34 of arg-1 with chest named "&6Mode Normal &0" with lore "&7Cliquez pour revenir en mode normal." and "&7" and "&eCliquez pour changer mode!" to run:
					set {SellItem-Mode-Normal.%UUID of player%} to true
					set {SellItem-Mode-Vision.%UUID of player%} to false
					play sound "block.chest.open" at player for player
					stop
					
				format gui slot 29 of arg-1 with map named "&9Vision du prix &0" with lore "&7Cliquez sur l'article en question en mode" and "&7vision pour voir le prix du vente." and "&7" and "&7Prix de l'Article: &6%{SellItem-Price.%UUID of arg-1%} * {SellItem-Mode-Size.%UUID of arg-1%}% &6Pièce(s)&7/%{SellItem-Mode-Size.%UUID of arg-1%}%"

on inventory click:
	if name of player's current inventory is "Joshua":
		cancel event
		if name of clicked item contains "&0":
			stop
		else:
		
